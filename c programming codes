#include<stdio.h>

 int main(){
   int a ,b;

     printf("Enter first number\n");

     scanf("%d",&a);

     printf("enter second number \n");

     scanf("%d",&b);

     printf("%d + %d = %d" , a , b , a+b);

     return 0;
 }
