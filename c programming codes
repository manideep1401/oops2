4th_
#include<stdio.h>

 int main(){
   int no1,no2,ans,op;

   printf("enter vlues of no1 and no2 : \n");
   scanf("%d%d" ,&no1, &no2);

   printf("1.addition \n");
   printf("2.substraction \n");
   printf("3.Multiplication \n");
   printf("4.division \n");
   printf("enter number for operation : \n");

   scanf("%d" ,&op);

   switch(op){

   case 1:ans = no1 +no2;
   printf("%d + %d = %d" , no1 ,no2, no1 + no2 , ans);
   break;

   case 2:ans = no1 - no2;
    printf("%d - %d = %d" , no1 ,no2, no1 - no2 , ans);
     break;

   case 3:ans = no1*no2;
     printf("%d * %d = %d" , no1 ,no2, no1 * no2 , ans);
       break;

   case 4:ans = no1/no2;
     printf("%d / %d = %d" , no1 ,no2, no1 / no2 , ans);
       break;
   }
    return 0;

 }
