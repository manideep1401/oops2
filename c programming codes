#include<stdio.h>

 int main(){
   int no1,no2;
   printf("Enter value of no1 : \n");
   scanf("%d ",&no1);

   printf("Enter value no2: \n");
   scanf("%d", & no2);

   if (no1 != no2){
      printf("Both numbers are not equal \n");

   if(no1 > no2){
     printf("no1 is greater  than no2 \n");
   }

   else{
   printf("no2 is not greter than no1");
   }}
   else{
   printf("no1 is reual to no2");
   }
   return 0;
 }
